{
  "name": "panicum",
  "version": "1.0.0",
  "description": "CSS Lightweight Framework",
  "main": "index.js",
  "scripts": {
    "dev:css": "parcel watch bundles/*.scss --no-source-maps -d dist/ --no-hmr --public-url ./",
    "build:css": "parcel build front/scss/*.scss --no-source-maps -d public/dist/css/ --public-url ./",
    "fsg": "./node_modules/.bin/fsg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinPucan/panicum.git"
  },
  "author": "MartinPucan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MartinPucan/panicum/issues"
  },
  "homepage": "https://github.com/MartinPucan/panicum#readme",
  "dependencies": {
    "file-structure-generator": "^1.0.5",
    "parcel": "^1.12.4"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "sass": "^1.29.0"
  }
}
