{
  "name": "panicum",
  "version": "1.0.3",
  "description": "CSS Lightweight Framework",
  "main": "index.js",
  "scripts": {
    "dev:css": "parcel watch src/bundles/*.scss --no-source-maps -d dist/ --no-hmr --public-url ./",
    "build:css": "parcel build src/bundles/*.scss --no-source-maps -d dist/ --public-url ./",
    "fsg": "./node_modules/.bin/fsg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinPucan/panicum.git"
  },
  "author": "MartinPucan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MartinPucan/panicum/issues"
  },
  "homepage": "https://github.com/MartinPucan/panicum#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "parcel": "^1.12.4"
  },
  "devDependencies": {
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.29.0",
    "sass-true": "^6.0.1"
  }
}
